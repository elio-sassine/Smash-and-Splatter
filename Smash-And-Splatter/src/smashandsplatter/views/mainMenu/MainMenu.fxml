<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="mainMenuPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="smashandsplatter.controller.mainMenu.MainMenuController">
   <children>
      <Button fx:id="smashBtn" layoutX="200.0" layoutY="430.0" mnemonicParsing="false" onAction="#handleSmash" prefHeight="70.4" prefWidth="230.4" />
      <Button fx:id="splatterBtn" layoutX="550.0" layoutY="430.0" mnemonicParsing="false" onAction="#handleSplatter" prefHeight="70.4" prefWidth="230.4" />
      <ImageView fx:id="titleImgView" fitHeight="500.0" fitWidth="500.0" layoutX="240.0" layoutY="-22.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/images/Title.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
